{
  "name": "raydium-info-extension",
  "displayName": "Raydium Info",
  "version": "0.0.1",
  "description": "Browser extension for Raydium information",
  "author": "Your Name",
  "scripts": {
    "dev": "plasmo dev --port 3000",
    "build": "plasmo build"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.11.0",
    "plasmo": "^0.60.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.200",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "typescript": "^4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://api-v3.raydium.io/*"
    ],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    }
  }
}
